{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@public/*": [
        "public/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@graphql/*": [
        "src/graphql/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@atoms/*": [
        "src/atoms/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "types/*": [
        "src/types/*"
      ]
    }
  }
}
